		Iterator<String> iterDev = Dev.iterator();
		
		if(Prod.size()== Dev.size()) {
			while(iterProd.hasNext()) {
				if(!iterProd.next().equals(iterDev.next())) {
					//funcion para correr y hacer los cambios 
				}
				else {
					//mostrar en log que esta igual 
				}
			}
		}
	}

}
